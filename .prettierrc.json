{
    "$schema": "https://json.schemastore.org/prettierrc",
    "printWidth": 120,
    "tabWidth": 2,
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": true,
    "semi": true,
    "useTabs": true,
    "arrowParens": "avoid",
    "endOfLine": "crlf",
    "quoteProps": "as-needed"
}
